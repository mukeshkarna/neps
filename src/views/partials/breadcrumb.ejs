<div class="page-title">
  <div class="row">
    <div class="col-12 col-md-6 order-md-1 order-last">
      <h3><%= (typeof title != "undefined" ? title : "Admin Dashboard") %></h3>
      <!-- <p class="text-subtitle text-muted">A pretty helpful component for give message to user</p> -->
    </div>
    <div class="col-12 col-md-6 order-md-2 order-first">
      <nav aria-label="breadcrumb" class='breadcrumb-header'>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
          <% if (breadcrumbs.length > 0) { %>
            <% breadcrumbs.forEach(function(breadcrumb, index){ %>
              <li class="breadcrumb-item <% if (breadcrumbs.length == 2 && index == 0 ) { %> active <% } %>" aria-current="page">
                <% if (breadcrumbs.length == 2 && index == 0 ) { %>
                  <a href="<%= breadcrumb.url %>"><%= breadcrumb.title%></a>
                <% }else { %>
                  <%= breadcrumb.title%>
                <% } %>
              </li>
            <% }); %>
          <% } %>
        </ol>
      </nav>
    </div>
  </div>

</div>


 